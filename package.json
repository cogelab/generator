{
  "name": "coge-generator",
  "version": "0.1.10",
  "description": "Coge generator auxiliary library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cogelab/generator.git"
  },
  "author": "Yuan Tao <towyuan@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cogelab/generator/issues"
  },
  "homepage": "https://github.com/cogelab/generator#readme",
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\" --single-quote --trailing-comma es5",
    "prebuild": "npm run clean && tsc --version",
    "build": "tsc --pretty",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "test": "jest"
  },
  "dependencies": {
    "@tiopkg/utils": "^2.1.8",
    "chalk": "^4.0.0",
    "coge-environment": "^0.5.3",
    "cross-spawn": "^7.0.2",
    "cross-spawn-promise": "^0.10.2",
    "dargs": "^7.0.0",
    "fs-extra": "^9.0.0",
    "github-username": "^5.0.1",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/shelljs": "^0.8.7",
    "@types/sinon": "^9.0.0",
    "jest": "^25.5.2",
    "nock": "^12.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/__tests__/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
